<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/coordinator_user_acc_details_setup"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="somame.amebleysystems.com.somame.CheckOutActivity">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar_check_out"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="?attr/colorPrimary"
            android:theme="@style/AppTheme2.NoActionBar"
            app:popupTheme="@style/AppTheme2.PopupOverlay"
            app:titleTextColor="@android:color/white"
            />
    <ProgressBar
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:theme="@style/AppTheme"
        android:id="@+id/progress_check_out"
        android:layout_gravity="center_horizontal|center_vertical"
        android:visibility="gone"
        />

        <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
                <LinearLayout
                    android:id="@+id/layout_check_out_linear"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginBottom="2dp">

                        <androidx.core.widget.NestedScrollView
                            android:id="@+id/nestedScrollView_check_out"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginTop="?attr/actionBarSize"
                            android:clipToPadding="false"
                            app:layout_behavior="@string/appbar_scrolling_view_behavior"
                            >

                        <LinearLayout
                            android:id="@+id/layout_check_out_all_fields"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_marginBottom="4dp">

                            <!--Personal Details-->
                            <androidx.appcompat.widget.AppCompatTextView xmlns:app="http://schemas.android.com/apk/res-auto"
                                android:id="@+id/textView_check_out_personal_details"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="8dp"
                                android:layout_marginBottom="1dp"
                                android:layout_marginStart="28dp"
                                app:autoSizePresetSizes="@array/autosize_text_tight1"
                                android:layout_gravity="start"
                                android:gravity="start"
                                app:autoSizeTextType="uniform"
                                android:textAlignment="center"
                                android:text="@string/personal_details"
                                android:visibility="visible"
                                android:textColor="@color/colorMyVioletPrimary"
                                />
                            <!--Full Name-->
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="2dp"
                                android:layout_marginStart="16dp"
                                android:layout_marginEnd="6dp"
                                android:orientation="horizontal"
                                android:layout_gravity="center_horizontal">
                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/ic_person"
                                    android:layout_marginTop="6dp"
                                    android:layout_gravity="center_vertical|start"
                                    android:contentDescription="@string/full_name" />
                                <com.google.android.material.textfield.TextInputLayout
                                    android:id="@+id/textInput_checkout_full_name"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">
                                    <AutoCompleteTextView
                                        android:id="@+id/edittext_check_out_full_name"
                                        android:layout_width="285dp"
                                        android:layout_height="wrap_content"
                                        android:hint="@string/full_name"
                                        android:inputType="text"
                                        android:maxLines="1"
                                        android:maxLength="30"
                                        android:singleLine="true"
                                        android:imeActionId="6"
                                        android:imeActionLabel="@string/full_name"
                                        android:imeOptions="actionUnspecified"
                                        app:layout_constraintHorizontal_bias="0.0"
                                        android:layout_gravity="center_vertical"
                                        android:layout_marginEnd="8dp"
                                        android:layout_marginStart="8dp"
                                        android:drawablePadding="10dp"
                                        android:paddingEnd="10dp"
                                        android:paddingStart="10dp"
                                        />
                                </com.google.android.material.textfield.TextInputLayout>
                                <CheckBox
                                    android:id="@+id/checkbox_check_out_full_name"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="end"
                                    android:checked="true"/>
                            </LinearLayout>

                            <!--Phone-->
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="2dp"
                                android:layout_marginStart="16dp"
                                android:layout_marginEnd="6dp"
                                android:orientation="horizontal"
                                android:layout_gravity="center_horizontal">
                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/ic_local_phone"
                                    android:layout_marginTop="6dp"
                                    android:layout_gravity="center_vertical"
                                    android:contentDescription="@string/phone" />
                                <com.google.android.material.textfield.TextInputLayout
                                    android:id="@+id/textInput_checkout_phone"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">
                                    <AutoCompleteTextView
                                        android:id="@+id/edittext_check_out_phone"
                                        android:layout_width="285dp"
                                        android:layout_height="wrap_content"
                                        android:hint="@string/phone"
                                        android:inputType="phone"
                                        android:maxLines="1"
                                        android:maxLength="10"
                                        android:singleLine="true"
                                        android:imeActionId="6"
                                        android:imeActionLabel="@string/phone"
                                        android:imeOptions="actionUnspecified"
                                        app:layout_constraintHorizontal_bias="0.0"
                                        android:layout_gravity="center_vertical"
                                        android:layout_marginEnd="8dp"
                                        android:layout_marginStart="8dp"
                                        android:drawablePadding="10dp"
                                        android:paddingEnd="10dp"
                                        android:paddingStart="10dp"
                                        />
                                </com.google.android.material.textfield.TextInputLayout>
                                <CheckBox
                                    android:id="@+id/checkbox_check_out_phone"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:checked="true"/>
                            </LinearLayout>

                            <androidx.appcompat.widget.AppCompatTextView xmlns:app="http://schemas.android.com/apk/res-auto"
                                android:id="@+id/textView_check_out_address"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="8dp"
                                android:layout_marginBottom="1dp"
                                android:layout_marginStart="28dp"
                                app:autoSizePresetSizes="@array/autosize_text_tight1"
                                android:layout_gravity="start"
                                android:gravity="start"
                                app:autoSizeTextType="uniform"
                                android:textAlignment="center"
                                android:text="@string/address_delivery_details"
                                android:visibility="visible"
                                android:textColor="@color/colorMyVioletPrimary"
                                />

                            <!--Address-->
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="2dp"
                                android:layout_marginStart="16dp"
                                android:layout_marginEnd="6dp"
                                android:orientation="horizontal"
                                android:layout_gravity="center_horizontal">
                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/ic_edit_location"
                                    android:layout_marginTop="6dp"
                                    android:layout_gravity="center_vertical"
                                    android:contentDescription="@string/address" />
                                <com.google.android.material.textfield.TextInputLayout
                                    android:id="@+id/textInput_checkout_address"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">
                                    <AutoCompleteTextView
                                        android:id="@+id/edittext_check_out_address"
                                        android:layout_width="285dp"
                                        android:layout_height="wrap_content"
                                        android:hint="@string/address"
                                        android:inputType="textMultiLine"
                                        android:maxLines="2"
                                        android:maxLength="50"
                                        android:lines="2"
                                        android:imeActionId="6"
                                        android:imeActionLabel="@string/address"
                                        android:imeOptions="actionUnspecified"
                                        app:layout_constraintHorizontal_bias="0.0"
                                        android:layout_gravity="center_vertical"
                                        android:layout_marginEnd="8dp"
                                        android:layout_marginStart="8dp"
                                        android:drawablePadding="10dp"
                                        android:paddingEnd="10dp"
                                        android:paddingStart="10dp"
                                        />
                                </com.google.android.material.textfield.TextInputLayout>
                                <CheckBox
                                    android:id="@+id/checkbox_check_out_address"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:checked="true"/>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="2dp"
                                android:layout_marginStart="16dp"
                                android:layout_marginEnd="6dp"
                                android:orientation="horizontal"
                                android:layout_gravity="center_horizontal">
                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/ic_gps"
                                    android:layout_marginTop="6dp"
                                    android:layout_gravity="center_vertical"
                                    android:contentDescription="@string/gh_post_gps_address" />
                                <com.google.android.material.textfield.TextInputLayout
                                    android:id="@+id/textInput_checkout_gh_post"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">
                                    <AutoCompleteTextView
                                        android:id="@+id/edittext_check_out_gh_post_address"
                                        android:layout_width="285dp"
                                        android:layout_height="wrap_content"
                                        android:hint="@string/gh_post_gps_address"
                                        android:inputType="text"
                                        android:maxLines="1"
                                        android:singleLine="true"
                                        android:maxLength="11"
                                        android:imeActionId="6"
                                        android:imeActionLabel="@string/gh_post_gps_address"
                                        android:imeOptions="actionUnspecified"
                                        app:layout_constraintHorizontal_bias="0.0"
                                        android:layout_gravity="center_vertical"
                                        android:layout_marginEnd="8dp"
                                        android:layout_marginStart="8dp"
                                        android:drawablePadding="10dp"
                                        android:paddingEnd="10dp"
                                        android:paddingStart="10dp"
                                        />
                                </com.google.android.material.textfield.TextInputLayout>
                                <CheckBox
                                    android:id="@+id/checkbox_check_out_gh_post_gps"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:checked="true"/>
                            </LinearLayout>

                            <!--Delivery Location-->
                            <androidx.appcompat.widget.AppCompatTextView xmlns:app="http://schemas.android.com/apk/res-auto"
                                android:id="@+id/textView_check_out_delivery_location"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="8dp"
                                android:layout_marginBottom="1dp"
                                android:layout_marginStart="28dp"
                                app:autoSizePresetSizes="@array/autosize_text_tight1"
                                android:layout_gravity="start"
                                android:gravity="start"
                                app:autoSizeTextType="uniform"
                                android:textAlignment="center"
                                android:text="@string/delivery_location"
                                android:visibility="visible"
                                android:textColor="@color/colorMyVioletPrimary"
                                />
                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="2dp"
                                android:orientation="horizontal"
                                >
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="16dp"
                                    android:layout_marginEnd="6dp"
                                    android:orientation="horizontal"
                                    android:layout_gravity="center_horizontal">
                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:src="@drawable/ic_location_on"
                                        android:layout_marginTop="6dp"
                                        android:layout_gravity="center_vertical"
                                        android:contentDescription="@string/delivery_location" />
                                    <Spinner
                                        android:id="@+id/spinner_check_out_delivery_location"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_horizontal"
                                        android:gravity="center_horizontal"/>
                                </LinearLayout>
                            </com.google.android.material.textfield.TextInputLayout>

                            <!--Delivery Period-->
                            <androidx.appcompat.widget.AppCompatTextView xmlns:app="http://schemas.android.com/apk/res-auto"
                                android:id="@+id/textView_check_out_delivery_period"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="start"
                                android:layout_marginBottom="1dp"
                                android:layout_marginStart="28dp"
                                android:layout_marginTop="8dp"
                                android:gravity="start"
                                android:text="@string/delivery_period"
                                android:textAlignment="center"
                                android:textColor="@color/colorMyVioletPrimary"
                                android:visibility="visible"
                                app:autoSizePresetSizes="@array/autosize_text_tight1"
                                app:autoSizeTextType="uniform" />
                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="0dp"
                                android:orientation="horizontal"
                                >
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="16dp"
                                    android:layout_marginEnd="6dp"
                                    android:orientation="horizontal"
                                    android:layout_gravity="center_horizontal">
                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:src="@drawable/ic_time"
                                        android:layout_marginTop="6dp"
                                        android:layout_gravity="center_vertical"
                                        android:contentDescription="@string/delivery_period" />
                                    <Spinner
                                        android:id="@+id/spinner_check_out_delivery_period"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_horizontal"
                                        android:gravity="center_horizontal"/>
                                </LinearLayout>
                            </com.google.android.material.textfield.TextInputLayout>

                            <!--Payment Method-->
                            <androidx.appcompat.widget.AppCompatTextView xmlns:app="http://schemas.android.com/apk/res-auto"
                                android:id="@+id/textView_check_out_payment_method"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="8dp"
                                android:layout_marginBottom="1dp"
                                android:layout_marginStart="28dp"
                                app:autoSizePresetSizes="@array/autosize_text_tight1"
                                android:layout_gravity="start"
                                android:gravity="start"
                                app:autoSizeTextType="uniform"
                                android:textAlignment="center"
                                android:text="@string/payment_method"
                                android:visibility="visible"
                                android:textColor="@color/colorMyVioletPrimary"
                                />
                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="2dp"
                                android:orientation="horizontal"
                                >
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="16dp"
                                    android:layout_marginEnd="6dp"
                                    android:orientation="horizontal"
                                    android:layout_gravity="center_horizontal">
                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:src="@drawable/ic_payment"
                                        android:layout_marginTop="6dp"
                                        android:layout_gravity="center_vertical"
                                        android:contentDescription="@string/payment_method" />
                                    <Spinner
                                        android:id="@+id/spinner_check_out_payment_method"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_horizontal"
                                        android:gravity="center_horizontal"/>
                                </LinearLayout>
                            </com.google.android.material.textfield.TextInputLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="2dp"
                                android:layout_marginStart="16dp"
                                android:layout_marginEnd="6dp"
                                android:orientation="horizontal"
                                android:layout_gravity="center_horizontal">
                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/ic_smartphone"
                                    android:layout_marginTop="6dp"
                                    android:layout_gravity="center_vertical"
                                    android:contentDescription="@string/momo_number" />
                                <com.google.android.material.textfield.TextInputLayout
                                    android:id="@+id/textInput_checkout_momo"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">
                                    <AutoCompleteTextView
                                        android:id="@+id/edittext_check_out_momo_number"
                                        android:layout_width="285dp"
                                        android:layout_height="wrap_content"
                                        android:hint="@string/momo_number"
                                        android:inputType="phone"
                                        android:maxLines="1"
                                        android:singleLine="true"
                                        android:maxLength="10"
                                        android:imeActionId="6"
                                        android:imeActionLabel="@string/momo_number"
                                        android:imeOptions="actionUnspecified"
                                        app:layout_constraintHorizontal_bias="0.0"
                                        android:layout_gravity="center_vertical"
                                        android:layout_marginEnd="8dp"
                                        android:layout_marginStart="8dp"
                                        android:drawablePadding="10dp"
                                        android:paddingEnd="10dp"
                                        android:paddingStart="10dp"
                                        />
                                </com.google.android.material.textfield.TextInputLayout>
                                <CheckBox
                                    android:id="@+id/checkbox_check_out_momo_number"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:checked="true"/>
                            </LinearLayout>

                        </LinearLayout>
                        </androidx.core.widget.NestedScrollView>
                </LinearLayout>
                    <!--Basket Summary Layout-->
                <androidx.core.widget.NestedScrollView
                    android:id="@+id/nested_scrollView_checkout_basket_summary"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:gravity="bottom">
                    <LinearLayout
                        android:id="@+id/layout_checkout_basket_summary"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:gravity="bottom">
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@color/colorGrayL"/>
                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardview_checkout_summary"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="8dp"
                            app:cardElevation="1dp"
                            >

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <androidx.appcompat.widget.AppCompatTextView xmlns:app="http://schemas.android.com/apk/res-auto"
                                    android:id="@+id/textview_checkout_basket_summary"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    app:autoSizePresetSizes="@array/autosize_text_tight1"
                                    android:layout_gravity="center_horizontal"
                                    app:autoSizeTextType="uniform"
                                    android:textAlignment="center"
                                    android:text="@string/basket_summary"
                                    android:layout_marginBottom="4dp"
                                    android:visibility="visible"
                                    android:textColor="@color/colorMyVioletLight"
                                    />

                                <!--Summary Begins-->
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical"
                                    android:padding="8dp"
                                    android:paddingStart="24dp"
                                    android:paddingEnd="24dp"
                                    >
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content">
                                        <TextView xmlns:app="http://schemas.android.com/apk/res-auto"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center_horizontal"
                                            android:gravity="center"
                                            android:textAlignment="center"
                                            android:text="@string/subtotal"
                                            android:textColor="@color/colorGray"
                                            />
                                        <TextView xmlns:app="http://schemas.android.com/apk/res-auto"
                                            android:id="@+id/textView_checkout_basket_subtotal"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:gravity="end"
                                            android:textAlignment="textEnd"
                                            android:text="@string/dummy_price"
                                            android:textStyle="bold"
                                            android:textColor="@color/colorGray"
                                            />
                                    </LinearLayout>
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal">
                                        <TextView xmlns:app="http://schemas.android.com/apk/res-auto"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center_horizontal"
                                            android:gravity="center"
                                            android:textAlignment="center"
                                            android:text="@string/delivery"
                                            android:textColor="@color/colorGray"
                                            />
                                        <TextView xmlns:app="http://schemas.android.com/apk/res-auto"
                                            android:id="@+id/textview_checkout_basket_shipping"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:gravity="end"
                                            android:textAlignment="textEnd"
                                            android:text="@string/dummy_price"
                                            android:textColor="@color/colorGray"
                                            android:textStyle="bold"
                                            />
                                    </LinearLayout>
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal">
                                        <TextView xmlns:app="http://schemas.android.com/apk/res-auto"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textAlignment="textStart"
                                            android:textStyle="bold"
                                            android:text="@string/total"
                                            android:textSize="16sp"
                                            android:textColor="@color/colorMyVioletPrimary"
                                            />
                                        <TextView xmlns:app="http://schemas.android.com/apk/res-auto"
                                            android:id="@+id/textview_checkout_basket_total"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="end"
                                            android:gravity="end"
                                            android:textAlignment="textEnd"
                                            android:text="@string/dummy_price"
                                            android:textColor="@color/colorMyVioletPrimary"
                                            android:textStyle="bold"
                                            android:textSize="16sp"
                                            />
                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical">
                                        <View
                                            android:layout_width="match_parent"
                                            android:layout_height="1dp"
                                            android:layout_marginStart="18dp"
                                            android:layout_marginEnd="18dp"
                                            android:layout_marginTop="8dp"
                                            android:layout_marginBottom="6dp"
                                            android:background="@color/colorGrayLl"
                                            />
                                        <Button
                                            android:id="@+id/button_check_out_next"
                                            android:layout_width="wrap_content"
                                            android:layout_height="35dp"
                                            android:layout_gravity="center_horizontal"
                                            android:background="@drawable/bg_button_continue_white"
                                            android:textColor="@color/colorMyVioletPrimary"
                                            android:text="@string/proceed"
                                            android:layout_marginBottom="2dp"
                                            />
                                    </LinearLayout>
                                </LinearLayout>
                            </LinearLayout>

                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                </androidx.core.widget.NestedScrollView>
                </RelativeLayout>
</RelativeLayout>
